{
	"[dart]": {
		"editor.defaultFormatter": "Dart-Code.dart-code",
		"editor.rulers": [80],
		"editor.selectionHighlight": false,
		"editor.suggest.snippetsPreventQuickSuggestions": false,
		"editor.wordBasedSuggestions": false
	},
	"[php]": {
		"editor.defaultFormatter": "kokororin.vscode-phpfmt"
	},
	"[python]": {},
	"cSpell.ignoreWords": ["Vitell"],
	"cSpell.userWords": [
		"apisauce",
		"browserslist",
		"chakra",
		"formik",
		"halim",
		"qtile",
		"qtilw",
		"tekturk"
	],
	"css.styleSheets": [
		"https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
	],
	"dart.debugExternalLibraries": false,
	"dart.debugSdkLibraries": false,
	"dart.warnWhenEditingFilesOutsideWorkspace": false,
	"debug.openDebug": "openOnDebugBreak",
	"editor.bracketPairColorization.enabled": true,
	"editor.codeLensFontFamily": "'FiraCode Nerd Font Mono','JetBrainsMono Nerd Font', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.foldingStrategy": "indentation",
	"editor.fontFamily": "'FiraCode Nerd Font Mono', 'JetBrainsMono Nerd Font', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
	"editor.fontLigatures": true,
	"editor.fontSize": 13,
	"editor.fontWeight": "450",
	"editor.formatOnSave": true,
	"editor.formatOnType": true,
	"editor.guides.bracketPairs": "active",
	"editor.linkedEditing": true,
	"editor.renderControlCharacters": true,
	"editor.semanticHighlighting.enabled": true,
	"editor.suggest.snippetsPreventQuickSuggestions": false,
	"editor.suggestSelection": "first",
	"editor.tabCompletion": "onlySnippets",
	"editor.tabSize": 2,
	"editor.tokenColorCustomizations": {
		"[Community Material Theme High Contrast]": {
			"comments": "#229977"
		}
	},
	"editor.wordBasedSuggestions": true,
	"editor.wordWrap": "on",
	"emmet.syntaxProfiles": {
		"javascript": "jsx",
		"php": "html",
		"xml": {
			"attr_quotes": "single"
		}
	},
	"eslint.alwaysShowStatus": true,
	"eslint.debug": true,
	"eslint.format.enable": true,
	"eslint.options": {
		"sort-keys": [
			"error",
			"asc",
			{
				"caseSensitive": true,
				"minKeys": 2,
				"natural": false
			}
		]
	},
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"extensions.autoUpdate": true,
	"files.associations": {
		"*.html": "html",
		"*.js": "javascriptreact"
	},
	"files.autoSave": "onFocusChange",
	"git.autofetch": true,
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"keyboard.dispatch": "keyCode",
	"liveServer.settings.donotShowInfoMsg": true,
	"phpfmt.passes": ["OnlyOrderUseClauses"],
	"prettier.prettierPath": "",
	"prettier.useTabs": true,
	"python.jediEnabled": false,
	"python.languageServer": "Default",
	"python.pythonPath": "/usr/bin/python3",
	"reactSnippets.settings.prettierEnabled": true,
	"sort-imports.default-sort-style": "renke",
	"sortJSON.contextMenu": {
		"sortJSON": true,
		"sortJSONAlphaNum": false,
		"sortJSONAlphaNumReverse": false,
		"sortJSONKeyLength": false,
		"sortJSONKeyLengthReverse": false,
		"sortJSONReverse": false,
		"sortJSONType": false,
		"sortJSONTypeReverse": false,
		"sortJSONValues": false,
		"sortJSONValuesReverse": false
	},
	"telemetry.telemetryLevel": "off",
	"terminal.integrated.fontFamily": "FiraCode Nerd Font",
	"terminal.integrated.fontSize": 13,
	"terminal.integrated.fontWeight": "500",
	"terminal.integrated.profiles.linux": {
		"bash": {
			"icon": "terminal-bash",
			"path": "bash"
		},
		"fish": {
			"path": "fish"
		},
		"pwsh": {
			"icon": "terminal-powershell",
			"path": "pwsh"
		},
		"tmux": {
			"icon": "terminal-tmux",
			"path": "tmux"
		},
		"zsh": {
			"path": "zsh"
		}
	},
	"terminal.integrated.profiles.windows": {
		"Command Prompt": {
			"args": [],
			"icon": "terminal-cmd",
			"path": [
				"${env:windir}\\Sysnative\\cmd.exe",
				"${env:windir}\\System32\\cmd.exe"
			]
		},
		"Git Bash": {
			"source": "Git Bash"
		},
		"PowerShell": {
			"icon": "terminal-powershell",
			"source": "PowerShell"
		}
	},
	"todo-tree.highlights.customHighlight": {
		"FIXME": {
			"background": "#ED254E",
			"foreground": "#DCDFDC",
			"gutterIcon": true,
			"iconColour": "#ED254E"
		},
		"REMEMBER": {
			"background": "#ED254E",
			"foreground": "#DCDFDC",
			"gutterIcon": true,
			"icon": "report",
			"iconColour": "#ED254E"
		},
		"TODO": {
			"background": "#2b72c9",
			"foreground": "#F9DC5C",
			"icon": "check",
			"iconColour": "#F9DC5C",
			"opacity": 80,
			"type": "line"
		}
	},
	"todo-tree.highlights.defaultHighlight": {
		"background": "white",
		"foreground": "red",
		"icon": "alert",
		"iconColour": "blue",
		"opacity": 50,
		"type": "text"
	},
	"todo-tree.ripgrep.ripgrep": "/home/ahmed/.cargo/bin/rg",
	"todo-tree.tree.showScanModeButton": false,
	"vim.camelCaseMotion.enable": true,
	"vim.enableNeovim": true,
	"vim.handleKeys": {
		"<C-d>": true
	},
	"vim.hlsearch": true,
	"vim.insertModeKeyBindings": [
		{
			"after": ["<Esc>"],
			"before": ["j", "j"]
		}
	],
	"vim.neovimPath": "/usr/bin/nvim",
	"vim.normalModeKeyBindings": [
		// {
		// 	"before": ["j"],
		// 	"after": ["g", "j"]
		// },
		// {
		// 	"before": ["k"],
		// 	"after": ["g", "k"]
		// }
	],
	"vim.useCtrlKeys": false,
	"vim.useSystemClipboard": true,
	"vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
	"vscode-neovim.neovimInitVimPaths.linux": "/home/ahmed/.config/VSCodium/User/init.vim",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"window.menuBarVisibility": "toggle",
	"workbench.colorCustomizations": {
		"[Community Material Theme Darker High Contrast]": {
			"statusBar.background": "#f50057",
			"statusBar.debuggingBackground": "#673ab7",
			"statusBar.foreground": "#fff",
			"statusBarItem.hoverBackground": "#ff80ab"
		},
		"terminal.ansiBlack": "#555555",
		"terminal.ansiBlue": "#75A1FF",
		"terminal.ansiBrightBlack": "#959DCB",
		"terminal.ansiBrightBlue": "#82AAFF",
		"terminal.ansiBrightCyan": "#676E95",
		"terminal.ansiBrightGreen": "#C3E88D",
		"terminal.ansiBrightMagenta": "#FFCB6B",
		"terminal.ansiBrightRed": "#F07178",
		"terminal.ansiBrightWhite": "#FFFEFE",
		"terminal.ansiBrightYellow": "#FF5572",
		"terminal.ansiCyan": "#60BAEC",
		"terminal.ansiGreen": "#62DE84",
		"terminal.ansiMagenta": "#F580FF",
		"terminal.ansiRed": "#F07178",
		"terminal.ansiWhite": "#ABB2BF",
		"terminal.ansiYellow": "#FFCB6B",
		"terminal.background": "#333333",
		"terminal.foreground": "#F2F2F2"
	},
	"workbench.colorTheme": "Pop Dark",
	"workbench.iconTheme": "material-icon-theme",
	"window.zoomLevel": 1,
	"typescript.updateImportsOnFileMove.enabled": "always"
}
