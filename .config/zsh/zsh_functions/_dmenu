#compdef dmenu

# zsh completions for 'dmenu'
# automatically generated with http://github.com/RobSis/zsh-completion-generator
local arguments

arguments=(
  '-b[dmenu appears at the bottom of the screen.]'
  '-f[dmenu grabs the keyboard before reading stdin if not  reading  from  a  tty.  This  is]'
  '-i[dmenu matches menu items case insensitively.]'
  '-l[dmenu lists items vertically, with the given number of lines.]'
  '-h[dmenu uses a menu line of at least height pixels tall, but no less than 8.]'
  '-m[dmenu  is  displayed on the monitor number supplied. Monitor numbers are starting from]'
  '-x[dmenu is placed at this offset measured from the left side of  the  monitor.   Can  be]'
  '-y[dmenu is placed at this offset measured from the top of the monitor.  If the -b option]'
  '-z[sets the width of the dmenu window.]'
  '-p[defines the prompt to be displayed to the left of the input field.]'
  '-fn[defines the font or font set used.]'
  '-nb[defines the normal background color.  #RGB, #RRGGBB, and X color names are supported.]'
  '-nf[defines the normal foreground color.]'
  '-sb[defines the selected background color.]'
  '-sf[defines the selected foreground color.]'
  '-v[prints version information to stdout, then exits.]'
  '-w[embed into windowid.]'
  '-H[save input in histfile and use it for history navigation.]'
  '*:filename:_files'
)

_arguments -s $arguments
